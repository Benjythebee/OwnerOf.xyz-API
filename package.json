{
  "name": "ownershipcheckerapi",
  "version": "0.0.1",
  "engines": {
    "node": ">=16"
  },
  "type": "commonjs",
  "scripts": {
    "clean": "rimraf dist/*",
    "format": "prettier --write \"src/**/*.ts\"",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "server": "node dist/server.js",
    "postbuild": "cp ./package.json ./package-lock.json ./dist/ && cd dist && npm install --only=production",
    "start": "npm-run-all dev:server",
    "dev:server": "ts-node-dev --respawn --watch ./src -- ./src/server.ts",
    "test": "tape -r ts-node/register ./tests/**/*.ts",
    "test:ci": "tape -r ts-node/register ./tests/**/*.ts | tap-xunit",
    "test:watch": "tape-watch -r ts-node/register ./tests/**/*.ts",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "docs":"apidoc -i src/ -o public/ -f \".*\\.ts$\" -c apidoc.json"
  },
  "author": "Benjamin Larcher",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "ethers": "^5.6.6",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "preact": "^10.6.6",
    "preact-render-to-string": "^5.1.20",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.182",
    "@types/node-fetch": "^2.6.1",
    "@types/rimraf": "^3.0.0",
    "events": "^3.3.0",
    "apidoc": "^0.51.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.4.3"
  }
}
